  "manifest_version": 3,
  "name": "Social Profile Launcher",
  "description": "Quick Launch Social Profile Links",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
